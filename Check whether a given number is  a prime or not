#include<stdlib.h>
#include<stdio.h>
#define MAx 100
int isPrime(int n)
{
 for(int i=2;i<n;i++)
 if(n % i == 0)
 return 0;
 return 1;
}
int main()
{
 int n,i,j=0;
 printf("Enter the number \n");
 scanf("%d",&n);
 i = isPrime(n);
 if(i==1)
 printf("Prime number\n");
 else
 printf("Not a prime number\n");
}
