#include<stdio.h>
int main()
{
   int a[20],n,i,j;
   printf("Enter the size of array :");
   scanf("%d",&n);
   printf("Enter the elements of array :");
   for(i=0;i<=n;i++)
     scanf("%d",&a[i]);
   printf("Twin Elements :");
   for(i=0;i<=n;i++)
   {
      for(j=i+1;j<n;j++)
      {
          if(a[i]==a[j])
          {
              printf("%d\n",a[i]);
          }
      }
   }
